<%= form_tag user_sessions_path, :method => :post do %>
  <div class="field-session">
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
</div>
<div class="field-session">
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
</div>
<div class="session-rmbr">
    <%= check_box_tag(:remember_me, class:"field-session-box") %><br />
    <%= label_tag :remember_me %>

</div>
  <div class="actions-session">
    <%= submit_tag("Login", class:"btn-session") %>
  </div>
<% end %>
