<%= form_for([@service, @job], html: {multipart: true}) do |f| %>

  <div class="field-job">
    <%= f.label :date %>
    <%= f.date_select :date %>



    <%= f.label :time %>
    <%= f.time_select :time %>


  <br>
    <%= f.label :notes %>
    <%= f.text_area :notes %>

</div>
  <br><div class="actions">
     <%= f.submit %>
  </div>


<br><% end %>
