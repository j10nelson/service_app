<%= form_for([@service, @job], html: {multipart: true}) do |f| %>

  <div class="field">
    <%= f.label :date %>
    <%= f.date_select :date %>
  </div>

  <div class="field">
    <%= f.label :time %>
    <%= f.time_select :time %>
  </div>

  <br><div class="field">
    <%= f.label :details %>
    <%= f.text_area :details %>
  </div>

  <br><div class="actions">
     <%= f.submit %>
  </div>

<br><% end %>
