<%= form_for([@service, @job], html: {multipart: true}) do |f| %>

  <div class="field-job">
    <%= f.label :date %>
    <%= f.date_select :date %>
  </div>

  <div class="field-job">
    <%= f.label :time %>
    <%= f.time_select :time %>
  </div>


  <br>
    <div class="field-job">
    <%= f.label :note %>
    <%= f.text_area :note %>
    </div>


  <br><div class="field-job">
     <%= f.submit(class: "btn-new-job") %>
  </div>


<br><% end %>
