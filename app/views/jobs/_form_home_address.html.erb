<%= form_for @user do |f| %>

<div class="field-session">
   <%= f.label :phone_number %><br />
   <%= f.text_field :phone_number %>
 </div>


  <div class="field-session">
     <%= f.label :house_number %><br />
     <%= f.text_field :house_number %>
   </div>

   <div class="field-session">
      <%= f.label :apt_number %><br />
      <%= f.text_field :apt_number %>
    </div>

   <div class="field-session">
      <%= f.label :street %><br />
      <%= f.text_field :street %>
    </div>

    <div class="field-session">
       <%= f.label :city %><br />
       <%= f.text_field :city %>
     </div>

    <div class="field-session">
    <%= f.select(:province) do %>
        <%= content_tag(:option, "Ontario", value: "ontario") %>
    <% end %>
  </div>

  <div class="field-session">
     <%= f.label :postal_code %><br />
     <%= f.text_field :postal_code %>
   </div>

    <div class="field-session">
    <%= f.select(:country) do %>
        <%= content_tag(:option, "Canada", value: "canada") %>
    <% end %>
  </div>

  <div class="actions-home-address">
     <%= f.submit("Complete Account", method: "put", class: "btn-session") %>
  </div>

<% end %>
