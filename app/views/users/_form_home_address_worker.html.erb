<%= form_for [@address] do |f| %>

  <div class="field-session">
     <%= f.label :house_number %><br />
     <%= f.text_field :house_number %>
   </div>

   <div class="field-session">
      <%= f.label :unit_number %><br />
      <%= f.text_field :unit_number %>
    </div>

   <div class="field-session">
      <%= f.label :street %><br />
      <%= f.text_field :street %>
    </div>

     <div class="field-session">
        <%= f.label :province %><br />
        <%= f.text_field :province %>
      </div>

      <div class="field-session">
         <%= f.label :country %><br />
         <%= f.text_field :country %>
       </div>

  <div class="actions-home-address">
     <%= f.submit("Complete Account", method: "post", class: "btn-session") %>
  </div>

<% end %>
